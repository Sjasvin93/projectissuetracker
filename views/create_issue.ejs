<html>
    <head>
        <title>
            <%= title %>
        </title>
        <!--Script to use fontawesome Icons-->
        <script src="https://kit.fontawesome.com/e1db25fb05.js" crossorigin="anonymous"></script>
        <!--Css file-->
        <link rel="stylesheet" href="/css/style.css">
    </head>
    <body>
        <div class="bug-container">
            <h2 class="bug_heading">REPORT BUG</h2>
            <form action="/bug/create-project-bug" method="post">
                <!-- <div > -->
                    <input type="text" placeholder="Enter the Title" name="bugTitle" class="bug-title" required>
                    <br>
                <!-- </div> -->
                <!-- <div > -->
                    <input type="text" placeholder="Enter the Description" name="bugDescription" class="bug-description"  required>
                    <br>
                <!-- </div> -->
                <!-- <div > -->
                    <input type="text" placeholder="Enter the label" name="bugLabel" class="bug-label" required>
                    <br>
                <!-- </div> -->
                <!-- <div > -->
                    <input type="text" placeholder="Author" name="bugAuthor" class="bug-author" required>
                    <br>
                <!-- </div> -->
                <!-- <div> -->
                    <input type="hidden" name="projectId" value="<%= project._id %>" >
                    <br>
                <!-- </div> -->
                <!-- <div class="bug-submit"> -->
                    <input type="submit" value="Submit" id="submit-bug-button">
                <!-- </div> -->
            </form>
        </div>
                
    </body>
</html>